{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc build active file",
			"command": "g++",
			"args": [
				"-std=c++17",
				"-ggdb3",
				"-Wall",
				"-Wextra",
				"-fpie",
				"-I/usr/include/python3.10",
				"-Wno-unused-result",
				"-Wsign-compare",
				"-fstack-protector-strong",
				"-Wformat",
				"-Werror=format-security",
				"-DNDEBUG",
				"-fdiagnostics-color=always",
				"-g",
				"${file}",
				"-L/usr/lib/python3.10/config-3.10-x86_64-linux-gnu",
				"-L/usr/lib/x86_64-linux-gnu",
				"-lpython3.10",
				"-lcrypt",
				"-o",
				"${fileDirname}/cnumpy"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: g++"
		}
	]
}